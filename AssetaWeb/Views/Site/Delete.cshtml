@model AssetaWeb.Models.SiteMasterTbl
@{
    ViewData["Title"] = "Delete";
}

<section class="content-header">
    <h1>
        Delete New Site Master
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"> Master</a></li>
        <li>Site Master</li>
        <li class="active">Delete Site Master</li>
    </ol>
    <br />

</section>
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content container-fluid">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title"></h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="col-sm-12">
                <form asp-action="Delete">
                    <div class="row form-border">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <h3>Delete This Site Master ?</h3><br/>
                            <div class="col-sm-6">

                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SiteId"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SiteId" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SiteId" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SiteCode"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SiteCode" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SiteCode" class="text-danger"></span>
                                <br />




                            </div>

                            <div class="col-sm-6">

                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SiteName"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SiteName" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SiteName" class="text-danger"></span>
                                <br />





                                <div class="row">
                                    <div class="col-sm-4">

                                    </div>
                                    <div class="col-sm-8">
                                        <input type="submit" value="Delete" class="btn btn-lg btn-primary" />
                                    </div>
                                </div><br />

                            </div>

                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.box-body -->
    </div>
</section>
