@model AssetaWeb.Models.TechnicianTbl

@{
    ViewData["Title"] = "Create";
}

<section class="content-header">
    <h1>
        Create New Technician Master
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"> Master</a></li>
        <li>Technician Master</li>
        <li class="active">Create New Data</li>
    </ol>
    <br />
</section>
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content container-fluid">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title"></h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="col-sm-12">
                <form asp-action="Create">
                    <div class="row form-border">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <div class="col-sm-3">
                                
                                
                            </div>

                            <div class="col-sm-6">

                                <div class="row">
                                    <div class="col-sm-4">
                                        <b>Technician Name</b>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="TechnicianName" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="TechnicianName" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                        <button type="submit" class="btn btn-default">
                                            <i class="far fa-save"></i>&nbsp;&nbsp;Create Data
                                        </button>
                                    </div>
                                </div><br />

                            </div>

                            <div class="col-sm-3">

                                
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.box-body -->
    </div>
</section>

