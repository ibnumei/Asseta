@model AssetaWeb.Models.SupplierTbl

@{
    ViewData["Title"] = "Create";
}

<section class="content-header">
    <h1>
        Create New Supplier Master
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"> Master</a></li>
        <li>Supplier Master</li>
        <li class="active">Create New Data</li>
    </ol>
    <br />
</section>
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content container-fluid">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title"></h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="col-sm-12">
                <form asp-action="Create">
                    <div class="row form-border">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SupplierCode"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SupplierCode" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SupplierCode" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Description"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Description" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Description" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                    </div>
                                </div><br />
                            </div>
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Address"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Address" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Address" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Contact"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Contact" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Contact" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="ProductData"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="ProductData" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="ProductData" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                        <input type="submit" value="Create" class="btn btn-lg btn-primary" />
                                    </div>
                                </div><br />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.box-body -->
    </div>
</section>
