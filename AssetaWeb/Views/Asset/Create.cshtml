@model AssetaWeb.Models.AssetTbl

@{
    ViewData["Title"] = "Create";
}

<section class="content-header">
    <h1>
        Create New Asset Master
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"> Master</a></li>
        <li>Asset Master</li>
        <li class="active">Create Asset New Data</li>
    </ol>
    <br />
</section>
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content container-fluid">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title"></h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="col-sm-12">
                <form asp-action="Create">
                    <div class="row form-border">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="AssetCode"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="AssetCode" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="AssetCode" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="AssetGroupId"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="AssetGroupId" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="AssetGroupId" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="AssetName"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="AssetName" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="AssetName" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SerialNumber"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SerialNumber" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SerialNumber" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="ValidityDate"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="ValidityDate" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="ValidityDate" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                    </div>
                                </div><br />
                            </div>
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="EntityId"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="EntityId" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="EntityId" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SiteId"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        @*<input asp-for="SiteId" class="form-control" autocomplete="off" />*@
                                        @Html.DropDownList("SITEID", null, htmlAttributes: new { @class = "form-control" })
                                    </div>
                                </div>
                                <span asp-validation-for="SiteId" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Location"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Location" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Location" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Valuation"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Valuation" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Valuation" class="text-danger"></span>
                                <br />

                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                        <input type="submit" value="Create" class="btn btn-lg btn-primary" />
                                    </div>
                                </div><br />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.box-body -->
    </div>
</section>