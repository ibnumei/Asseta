@model AssetaWeb.Models.EntityTbl

@{
    ViewData["Title"] = "Create";
}

<section class="content-header">
    <h1>
        Create New Entity Master
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"> Master</a></li>
        <li>Entity Master</li>
        <li class="active">Create Entity New Data</li>
    </ol>
    <br />
</section>
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content container-fluid">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title"></h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="col-sm-12">
                <form asp-action="Create">
                    <div class="row form-border">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="EntityCode"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="EntityCode" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="EntityCode" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="EntityName"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="EntityName" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="EntityName" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="SiteId"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="SiteId" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="SiteId" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Contact"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Contact" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Contact" class="text-danger"></span>
                                <br />


                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                    </div>
                                </div><br />
                            </div>
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="Pic"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="Pic" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="Pic" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="CreatedBy"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="CreatedBy" class="form-control" autocomplete="off" />
                                       @*@Html.DropDownList("SITEID", null, htmlAttributes: new { @class = "form-control" })*@
                                    </div>
                                </div>
                                <span asp-validation-for="CreatedBy" class="text-danger"></span>
                                <br />
                                <div class="row">
                                    <div class="col-sm-4">
                                        <label asp-for="ModifyBy"></label>
                                    </div>
                                    <div class="col-sm-8">
                                        <input asp-for="ModifyBy" class="form-control" autocomplete="off" />
                                    </div>
                                </div>
                                <span asp-validation-for="ModifyBy" class="text-danger"></span>
                                <br />


                                <div class="row">
                                    <div class="col-sm-4">
                                    </div>
                                    <div class="col-sm-8">
                                        <input type="submit" value="Create" class="btn btn-lg btn-primary" />
                                    </div>
                                </div><br />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /.box-body -->
    </div>
</section>

